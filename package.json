{
  "name": "cars",
  "description": "A Web API server & CLI for counting and reporting your data",
  "version": "1.0.0",
  "author": {
    "name": "bouzuya",
    "mail": "m@bouzuya.net",
    "url": "http://bouzuya.net"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6"
          }
        }
      ],
      "power-assert"
    ]
  },
  "bin": {
    "cars-api": "./bin/cars-api",
    "cars": "./bin/cars"
  },
  "bugs": {
    "url": "https://github.com/bouzuya/cars/issues"
  },
  "dependencies": {
    "cars-counter-bbn-entries": "https://github.com/bouzuya/cars-counter-bbn-entries",
    "cars-counter-github": "https://github.com/bouzuya/cars-counter-github",
    "cars-counter-hatena-bookmark": "https://github.com/bouzuya/cars-counter-hatena-bookmark",
    "cars-counter-twitter": "https://github.com/bouzuya/cars-counter-twitter",
    "cars-counter-wakeup": "https://github.com/bouzuya/cars-counter-wakeup",
    "cars-reporter-console": "https://github.com/bouzuya/cars-reporter-console",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@types/express": "^4.16.1",
    "@types/node": "^11.10.4",
    "@types/power-assert": "^1.5.0",
    "babel-preset-power-assert": "^3.0.0",
    "beater": "^5.0.1",
    "coveralls": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.3.0",
    "power-assert": "^1.6.1",
    "rimraf": "^2.6.3",
    "sinon": "^7.2.7",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333",
    "watch": "^1.0.2"
  },
  "homepage": "https://github.com/bouzuya/cars",
  "keywords": [
    "shuburi"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bouzuya/cars"
  },
  "scripts": {
    "build": "npm-run-all -s build:lint build:es2015 build:es5 build:lib",
    "build:es2015": "tsc",
    "build:es5": "babel --out-dir .tmp/es5/ --source-maps inline .tmp/es2015/",
    "build:lib": "babel --copy-files --no-comments --out-dir ./lib/ .tmp/es2015/src/",
    "build:lint": "tslint --project tsconfig.json",
    "clean": "rimraf .nyc_output .tmp coverage lib",
    "coveralls": "nyc report --reporter text-lcov | coveralls",
    "prepare": "npm-run-all -s 'clean' 'build'",
    "start": "./bin/cars-api",
    "test": "nyc --reporter lcov node .tmp/es5/test/index.js && nyc report",
    "watch": "npm-run-all -p 'watch:*'",
    "watch:es2015": "npm run build:es2015 -- --watch",
    "watch:es5": "npm run build:es5 -- --watch",
    "watch:lib": "npm run build:lib -- --watch",
    "watch:test": "watch 'npm run test' .tmp/es5/ --wait 2",
    "worker": "./bin/cars"
  }
}
